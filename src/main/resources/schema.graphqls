type Query {
    metroline(token: ID!): Metroline
}

type  Metroline {
    stations: [Station]
}

type  Station {
    id: String
    content: StationContent
}

union StationContent = OrderCreationContent | RequestCreationContent

type OrderCreationContent {
    orderId: String
}

type RequestCreationContent {
    requestId: String
}

